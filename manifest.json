{
  "manifest_version": 3,
  "name": "标签选择器",
  "description": "标签选择器",
  "version": "0.0.4",
  "icons": {
    "128": "icon_128.png"
  },
  "action": {
    "default_icon": "icon_128.png",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "storage"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "mac": "Command+E"
      }
    }
  },
  "_execute_action": {
    "suggested_key": {
      "windows": "Ctrl+E",
      "mac": "Command+E",
      "chromeos": "Ctrl+E",
      "linux": "Ctrl+E"
    }
  }
}
